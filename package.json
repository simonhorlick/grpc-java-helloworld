{
  "name": "helloworld",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/google-protobuf": "^3.2.6",
    "@types/react": "^15.0.33",
    "@types/react-dom": "^15.5.1",
    "jest": "^20.0.4",
    "react-dom": "^15.6.1",
    "ts-loader": "^2.1.0",
    "ts-protoc-gen": "^0.3.2",
    "typescript": "^2.4.1",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2"
  },
  "dependencies": {
    "google-protobuf": "^3.3.0",
    "grpc-web-client": "^0.2.1",
    "react": "^15.6.1"
  },
  "scripts": {
    "build:proto": "./generate_javascript.sh",
    "build": "npm run build:proto && webpack --define process.env.NODE_ENV='\"development\"'",
    "build_prod": "npm run build:proto && webpack -p --define process.env.NODE_ENV='\"production\"'",
    "webpack-dev": "export USE_TLS=true && webpack-dev-server --colors --watch --hot --inline --port 8082 --host 0.0.0.0 --content-base ./js --output-public-path=https://localhost:8082/ --https --cert=./certs/localhost.pem --key=./certs/localhost-key.pem",
    "start": "npm run build:proto && npm run webpack-dev",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testRegex": ".*\\.test\\.tsx?$"
  }
}
